#include <bits/stdc++.h>
using namespace std;
int main() {
	int t;
    cin>>t;
	while(t--){
		int n;
		cin>>n;
		long long sum=1;
		for(int i=1;i<=n;i++){
			int x;
			cin>>x;
			sum=sum*x;
		}
		int r=sum%10;
		if(r==2 || r==3 || r==5){
			cout<<"YES"<<endl;
		}
		else{
			cout<<"NO"<<endl;
		}
	}
}
